{{yield}}

<h2>{{title}}</h2>


{{#paper-content class="md-whiteframe-z1 peoples"}}
  {{#paper-list}}
    {{#each people as |person|}}
      {{#paper-item class="md-3-line"}}
        <img src="https://ipfs.pics/ipfs/QmZBRQ4geXNa4rDBdvAMidYvkpTxgwfiKqUvabsn8QBJE3" class="md-avatar">
        <div class="md-list-item-text">
          <h3>{{person}}</h3>
        </div>
      {{/paper-item}}
    {{/each}}
  {{/paper-list}}
{{/paper-content}}
