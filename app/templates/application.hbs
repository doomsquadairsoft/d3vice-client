{{#paper-nav-container}}
  {{#paper-sidenav class="md-sidenav-left md-whiteframe-z2" locked-open="gt-sm"}}
    {{#paper-toolbar}}
      <div class="md-toolbar-tools">
        <div class="logo">
          <img src="device-logo-white.png" alt="D3VICE" height="30"/>
        </div>
      </div>
    {{/paper-toolbar}}

    {{#paper-content}}
      {{#paper-list}}
        {{#paper-item action="transitionTo" param="index"}}Home{{/paper-item}}
        {{#paper-item action="transitionTo" param="game"}}Game{{/paper-item}}
        {{#paper-item action="transitionTo" param="nodes"}}Nodes{{/paper-item}}
      {{/paper-list}}
    {{/paper-content}}
  {{/paper-sidenav}}

  <div layout="column" tabindex="-1" role="main" flex>
    {{outlet}}
  </div>
{{/paper-nav-container}}
